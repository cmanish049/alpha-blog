<h1>Article Listing Page</h1>

<table>
<thead>
<tr>
<th>Title</th>
<th>Descrption</th>
<th>Actions</th>
</tr>
<thead>
<tbody>
    <% @articles.each do |article| %>
    <tr>
        <td><%= article.title %></td>
        <td><%= article.description %></td>
        <td>
            <%= link_to 'Show', article %> 
            <%= link_to 'Edit', edit_article_path(article) %> 
            <%= link_to 'Delete', article, method: :delete %>
        </td>
    </tr>
    <% end %>
</tbody>
</table>